{
    "bin": {"texteditor": "jdeploy-bundle/jdeploy.js"},
    "author": "Steve Hannah",
    "description": "A text editor",
    "main": "index.js",
    "preferGlobal": true,
    "repository": {"url": "https://github.com/shannah/jdeploy-demo-texteditor2"},
    "version": "0.0.0-JDEPLOY-SNAPSHOT",
    "jdeploy": {
        "installers": [
            "mac-arm64",
            "win",
            "linux"
        ],
        "javaVersion": "11",
        "title": "Text Editor",
        "downloadPage": {"platforms": ["default"]},
        "args": ["-Dfoo=bar"],
        "fallbackToUniversal": false,
        "publishTargets": [],
        "permissions": [{
            "name": "photos_add",
            "description": "Needs access to photos"
        }],
        "documentTypes": [
            {
                "editor": true,
                "extension": "txt",
                "mimetype": "text/plain"
            },
            {
                "extension": "html",
                "mimetype": "text/html"
            },
            {
                "extension": "jdtext",
                "mimetype": "application/x-jdeploy-demo-texteditor-jdtext"
            },
            {
                "editor": true,
                "extension": "html2",
                "custom": true,
                "mimetype": "text/html2"
            },
            {
                "role": "Editor",
                "description": "Open as project",
                "type": "directory"
            }
        ],
        "urlSchemes": [
            "jdtext",
            "myjdtext"
        ],
        "jdkProvider": "jbr",
        "jar": "target/TextEditor-1.0-SNAPSHOT.jar",
        "javafx": true,
        "jbrVariant": "jcef",
        "platformBundlesEnabled": false
    },
    "dependencies": {
        "command-exists-promise": "^2.0.2",
        "node-fetch": "2.6.7",
        "tar": "^4.4.8",
        "yauzl": "^2.10.0",
        "shelljs": "^0.8.4"
    },
    "license": "ISC",
    "name": "jdeploy-demo-texteditor",
    "files": ["jdeploy-bundle"],
    "scripts": {"test": "echo \"Error: no test specified\" && exit 1"}
}